@using proje1.BusinessLayer
@using proje1.Entities
@{ 
    List<Sepet> spts = new List<Sepet>();
    List<Sepet> sepetler = new List<Sepet>();
    long tfiyat=0;
    Kullanici klnc = Session["giris"] as Kullanici;
    Kullanici admin = null;
    if (klnc!=null && klnc.adminmi)
    {
        admin = klnc;
    }
    if (klnc != null)
    {
        SepetYonetici sy = new SepetYonetici();
        UrunYonetici uy = new UrunYonetici();
        Urun urn = new Urun();
        spts = sy.List(x => x.kullaniciId == klnc.kullaniciId);
        if (spts.Count > 0)
        {
            sepetler = new List<Sepet>();
            foreach (Sepet spt in spts)
            {
                if (spt.faturaId == null)
                {
                    urn = uy.Find(x => x.urunId == spt.urunId);
                    sepetler.Add(spt);
                    tfiyat += urn.urunFiyati;
                }
            }
        }
    }
}
