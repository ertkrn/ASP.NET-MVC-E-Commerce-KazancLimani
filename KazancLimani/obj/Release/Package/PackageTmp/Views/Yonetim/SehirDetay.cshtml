@model proje1.Entities.Sehir

@{
    ViewBag.Title = "KazancLimani - Şehir Detayı";
    Layout = "~/Views/Shared/_LayoutPage3.cshtml";
}
<div class="panel-heading">
    <h2>@Model.ilAdi.ToUpper() ŞEHRİNİN DETAYI</h2>
</div>
<div class="panel-body">
    <h4>Sehir</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ilPlaka)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ilPlaka)
        </dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ilAdi)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ilAdi)
        </dd>
    </dl>
</div>
<div class="panel-footer">
    <p>
        @Html.ActionLink("Düzenle", "SehirDuzenle", new { id = Model.ilPlaka }) |
        @Html.ActionLink("Geri Dön", "SehirYonetim")
    </p>
</div>
